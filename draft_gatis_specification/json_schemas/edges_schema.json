{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/dotbts/BPA/tree/main/draft_gatis_specification/edges_schema.json",
    "title": "Draft GATIS edges.geojson JSON-Schema",
    "type": "object",
    "required": [
        "type",
        "features"
    ],
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "FeatureCollection"
            ]
        },
        "features": {
            "type": "array",
            "items": {
                "title": "GeoJSON Feature",
                "type": "object",
                "required": [
                    "type",
                    "properties",
                    "geometry"
                ],
                "properties": {
                    "type": {
                        "type": "string",
                        "enum": [
                            "Feature"
                        ]
                    },
                    "id": {
                        "oneOf": [
                            {
                                "type": "number"
                            },
                            {
                                "type": "string"
                            }
                        ]
                    },
                    "properties": {
                        "edge_id": {
                            "type": "string"
                        },
                        "road_associated": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "reference_ids": {
                            "oneOf": [
                                {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {}
                                    }
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "street_name": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "facility_name": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "edge_type": {
                            "type": "string",
                            "enum": [
                                "road",
                                "sidewalk",
                                "footpath",
                                "crossing",
                                "traffic_island",
                                "steps",
                                "escalator",
                                "bikeway",
                                "multi_use_path",
                                "trail",
                                "virtual_link"
                            ]
                        },
                        "from_node": {
                            "type": "string"
                        },
                        "to_node": {
                            "type": "string"
                        },
                        "directionality": {
                            "type": "string",
                            "enum": [
                                "forward",
                                "backward",
                                "both"
                            ]
                        },
                        "width": {
                            "type": "number",
                            "minimum": 0
                        },
                        "width_min": {
                            "type": "number",
                            "minimum": 0
                        },
                        "width_tolerance": {
                            "type": "number",
                            "minimum": 0
                        },
                        "bikeway_type": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "bikeway_grade_separation": {
                            "type": "string",
                            "enum": [
                                "at_grade",
                                "raised",
                                "sidewalk_level"
                            ]
                        },
                        "separation_elements": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "separation_permeable_car": {
                            "type": "string",
                            "enum": [
                                "hard separator:  the separator cannot be easily bypassed by motor vehicles (jersey barriers",
                                "curbs)",
                                "soft separator: the separator can be easily bypassed by motor vehicles (flex posts",
                                "k-rail)",
                                "none: no separator is present (just paint separation)"
                            ]
                        },
                        "buffer_width": {
                            "type": "number"
                        },
                        "street_parking": {
                            "type": "string",
                            "enum": [
                                "parallel",
                                "angled",
                                "floating: Also known as parking protected. Put this value if present regardless if parking is parallel/angled parking."
                            ]
                        },
                        "street_parking_buffer": {
                            "type": "number"
                        },
                        "traffic_volume": {
                            "type": "number",
                            "minimum": 0
                        },
                        "posted_speed_limit": {
                            "type": "number",
                            "minimum": 0
                        },
                        "car_freeflow_speed": {
                            "type": "number",
                            "minimum": 0
                        },
                        "thru_lanes": {
                            "type": "number",
                            "minimum": 0
                        },
                        "aux_lanes": {
                            "type": "number",
                            "minimum": 0
                        },
                        "shoulder_width": {
                            "type": "number"
                        },
                        "roadway_centerline": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "bridge": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "mup_modal_delineation": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "prohibited_uses": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [
                                    "scramble / all pedestrian",
                                    "leading pedestrian interval",
                                    "no right on red",
                                    "none",
                                    "unknown"
                                ]
                            }
                        },
                        "allowed_uses": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [
                                    "scramble / all pedestrian",
                                    "leading pedestrian interval",
                                    "no right on red",
                                    "none",
                                    "unknown"
                                ]
                            }
                        },
                        "surface_material": {
                            "type": "string",
                            "enum": [
                                "asphalt",
                                "concrete",
                                "gravel",
                                "grass",
                                "dirt",
                                "paved",
                                "unpaved",
                                "grass_paver",
                                "paving_stones",
                                "other"
                            ]
                        },
                        "surface_issue": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "status": {
                            "type": "string",
                            "enum": [
                                "open",
                                "closed",
                                "under construction",
                                "unknown"
                            ]
                        },
                        "seasonal": {
                            "oneOf": [
                                {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {}
                                    }
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "pedestrian_lane": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "incline": {
                            "type": "number"
                        },
                        "cross_slope": {
                            "type": "number"
                        },
                        "cross_slope_max": {
                            "type": "number"
                        },
                        "ada_compliance": {
                            "type": "string",
                            "enum": [
                                "yes",
                                "no",
                                "unknown"
                            ]
                        },
                        "ada_compliance_date": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "format": "date"
                                },
                                {
                                    "type": "string",
                                    "pattern": "^[1-9]\\d{3}-(?:0[1-9]|1[0-2])$"
                                },
                                {
                                    "type": "string",
                                    "pattern": "^[1-9]\\d{3}$"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "ada_compliance_standard": {
                            "type": "string",
                            "enum": [
                                "2010",
                                "PROWAG"
                            ]
                        },
                        "impediment": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "step_count": {
                            "type": "number",
                            "minimum": 0
                        },
                        "handrail": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "wheel_channel": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "date_built": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "format": "date"
                                },
                                {
                                    "type": "string",
                                    "pattern": "^[1-9]\\d{3}-(?:0[1-9]|1[0-2])$"
                                },
                                {
                                    "type": "string",
                                    "pattern": "^[1-9]\\d{3}$"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "check_date": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "format": "date"
                                },
                                {
                                    "type": "string",
                                    "pattern": "^[1-9]\\d{3}-(?:0[1-9]|1[0-2])$"
                                },
                                {
                                    "type": "string",
                                    "pattern": "^[1-9]\\d{3}$"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "traffic_calming": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        },
                        "curb_height": {
                            "type": "number",
                            "minimum": 0
                        },
                        "official": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "presence": {
                            "type": "string",
                            "enum": [
                                "yes",
                                "no",
                                "missing",
                                "unknown"
                            ]
                        },
                        "rail": {
                            "oneOf": [
                                {
                                    "type": "string",
                                    "enum": [
                                        "yes",
                                        "no"
                                    ]
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "visual_markings": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "detectable_warning": {
                            "type": "string",
                            "enum": [
                                "yes",
                                "no",
                                "unknown"
                            ]
                        },
                        "vehicle_traffic_control": {
                            "type": "string",
                            "enum": [
                                "uncontrolled",
                                "standard signal",
                                "flashing red signal",
                                "flashing yellow signal",
                                "yield sign",
                                "stop sign"
                            ]
                        },
                        "cross_vehicle_traffic_control": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [
                                    "scramble / all pedestrian",
                                    "leading pedestrian interval",
                                    "no right on red",
                                    "none",
                                    "unknown"
                                ]
                            }
                        },
                        "ped_traffic_control": {
                            "type": "string",
                            "enum": [
                                "Uncontrolled",
                                "standard signal",
                                "flashing red signal",
                                "flashing yellow signal",
                                "pedestrian hybrid beacon / HAWK",
                                "rectangular rapid flashing beacon",
                                "yield sign",
                                "stop sign"
                            ]
                        },
                        "ped_protection": {
                            "type": "array",
                            "items": {
                                "type": "string",
                                "enum": [
                                    "scramble / all pedestrian",
                                    "leading pedestrian interval",
                                    "no right on red",
                                    "none",
                                    "unknown"
                                ]
                            }
                        },
                        "measured_length": {
                            "type": "number"
                        }
                    },
                    "geometry": {
                        "title": "GeoJSON LineString",
                        "type": "object",
                        "required": [
                            "type",
                            "coordinates"
                        ],
                        "properties": {
                            "type": {
                                "type": "string",
                                "enum": [
                                    "LineString"
                                ]
                            },
                            "coordinates": {
                                "type": "array",
                                "minItems": 2,
                                "items": {
                                    "type": "array",
                                    "minItems": 2,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            },
                            "bbox": {
                                "type": "array",
                                "minItems": 4,
                                "items": {
                                    "type": "number"
                                }
                            }
                        }
                    },
                    "bbox": {
                        "type": "array",
                        "minItems": 4,
                        "items": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "bbox": {
            "type": "array",
            "minItems": 4,
            "items": {
                "type": "number"
            }
        }
    },
    "description": "Draft JSON-Schema for GATIS edges",
    "additionalProperties": false
}